<div class="max-w-3xl mx-auto px-4">
  <div class="flex items-center justify-between">
    <h1 class="flex-1 font-bold text-3xl">End_user</h1>

    <div class="flex-wrap lg:flex items-center lg:space-x-3">
      <%= link_to end_users_path, class: button_class(theme: "transparent") do %>
      All End_users
      <% end %>
      <%= link_to edit_end_user_path(@end_user), class: button_class(theme:"primary") do %>
      Edit
      <% end %>
    </div>
  </div>

  <ul class="text-slate-700">
      <li>
        <p class="font-semibold">Username:</p>
        <p><%= @end_user.username %></p>
      </li>
      <li>
        <p class="font-semibold">Date of birth:</p>
        <p><%= @end_user.date_of_birth %></p>
      </li>
      <li>
        <p class="font-semibold">Email:</p>
        <p><%= @end_user.email %></p>
      </li>
  </ul>
</div>
